<div class="imageback">
  <div class="container">
    <div class="container-form">
      <h1>Add Album:</h1>
      <%= simple_form_for (@album) do |f| %>
        <%= f.input :title %>
        <%= f.input :genre %>
        <%= f.input :picture %>
        <%= f.submit class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
